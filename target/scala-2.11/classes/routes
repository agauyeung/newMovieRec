# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Recommended For You page
GET     /recommended                controllers.Application.recommended()

# Rating History page
GET     /history                    controllers.Application.history()

# User_DB Registration page
GET     /register_user                    controllers.Application.register_user()


GET		/view				controllers.Application.view()

# View users in the database
GET		/users				controllers.Application.users()	

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)


# Ratings
GET     /rate                       controllers.Application.rate()

# Results
POST    /results                    controllers.Application.results()

GET     /random                    controllers.Application.random()

POST     /registered                 controllers.Application.registered()

#Login

# GET     /login                  controllers.Application.login()
